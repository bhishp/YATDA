{
  "name": "YATDA",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pretest": "echo 'running YATDA tests'",
    "test": "tape ./test/**/*.tap.js",
    "test-cover": "istanbul cover node_modules/tape/bin/tape ./test/**/*.tap.js",
    "test-coveralls": "istanbul cover node_modules/tape/bin/tape ./test/**/*.tap.js && cat ./coverage/lcov.info | ./node_modules/coveralls.js",
    "postinstall": "echo 'Thank-you for installing YATDA app'",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "csv": "^0.4.6",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.1",
    "rimraf": "^2.4.4",
    "tape": "^4.2.2"
  }
}
